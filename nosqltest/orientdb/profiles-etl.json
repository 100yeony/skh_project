{
  "config": {
    "parallel": false
  },
  "source": { "file": { "path": "@INPUT_PROFILES@" } },
  "extractor": { "row": {} },
  "transformers": [
    { "csv": { "separator": "|", "stringCharacter": "", "unicode": false } },
    { "vertex": { "class": "Profile_ID" } }
  ],
  "loader": {
    "orientdb": {
       "dbURL": "plocal:@DATABASE@",
       "dbType": "graph",
       "wal": false,
       "batchCommit": 10000,
       "tx": false,
       "useLightweightEdges" : true,
       "dbAutoCreateProperties": true,
       "classes": [
         {"name": "Profile", "extends": "V", "clusters": 1},
         {"name": "Relation", "extends": "E"}
       ], "indexes": [
         {"class":"Profile_ID", "fields":["_key:string"], "type":"UNIQUE_HASH_INDEX" }
       ],
       "settings": {
       }
    }
  }
}
